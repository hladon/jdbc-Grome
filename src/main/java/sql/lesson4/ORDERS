CREATE TABLE ORDERS(
ID INT,
CONSTRAINT ORDER_ID PRIMARY KEY (ID),
USER_ID INT,
CONSTRAINT USER_FK FOREIGN KEY (USER_ID)
REFERENCES USERS(ID),
ROOM_ID INT,
CONSTRAINT ROOM1_FK FOREIGN KEY (ROOM_ID)
REFERENCES ROOMS(ID),
DATE_FROM TIMESTAMP,
DATE_TO TIMESTAMP,
MONEY_PAID NUMBER(8,2)
);
CREATE SEQUENCE O_SEQ MINVALUE 1 MAXVALUE 100 START WITH 1 INCREMENT BY 1 ;